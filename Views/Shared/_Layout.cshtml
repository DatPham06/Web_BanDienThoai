<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/StyleCSS/StyleSheet1.css" rel="stylesheet" />
</head>

<body>
    <div id="heading">
        <div id="header-left">
            <img src="~/Picture/LOGO_HUFLIT.png" class="logo" />
            <div id="storename">
                <h2>PhoneStoreGR2</h2>
            </div>
        </div>
        <div id="header-mid">
            <input type="text" placeholder="nhập từ khóa" class="search-box" />
            <button type="submit" class="search-btn">Tìm kiếm</button>
            <ul id="menu">
                <li>Trang Chủ</li>
                <li>Sản Phẩm</li>
                <li>Liên Hệ</li>
            </ul>
        </div>
        <div id="header-right">

        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <footer>
    </footer>
</body>
</html>